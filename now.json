{
  "name": "hydroponics",
  "alias": ["hydroponics.cass.si"],
  "builds": [{
    "src": "frontend/package.json",
    "use": "@now/static-build",
    "config": {"destDir":"build/"}
   }],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "http://api.hydroponics.cass.si/$1", "continue":false },
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ]
}
